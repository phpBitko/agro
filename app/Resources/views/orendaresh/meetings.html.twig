                    <div class="meet_item_content">
                        <div class="meet_item_header">
                            <div class="meet_ico">
                            </div>
                            <div class="meet_content">
                                <span class="title">{{ meeting.title }}</span>
                                <span class="decr">Комісія відбулася {{ meeting.closed|date("d/m/Y h:m") }}</span>
                            </div>
                            <div class="status">Опубліковано - </div>
